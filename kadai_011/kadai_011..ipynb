    "#kadai_011\n",
    "array = [\"火\",\"金\",\"地\",\"水\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
    "\n",
    "count = 1   #連番になるようにカウント\n",
    "\n",
    "#forループで水～冥の9行を表示\n",
    "for i in range(len(array)):\n",
    "    print(count , array[i])\n",
    "    count += 1\n",
    "    \n",
    "#whileループで水～冥の9行を表示\n",
    "i = 0\n",
    "while i < len(array):\n",
    "    print(count , array[i])\n",
    "    count += 1\n",
    "    i += 1"
   ]
