    "class Human: #クラス定義\n",
    "    def __init__(self, name, age):\n",
    "        self.name = name \n",
    "        self.age = age   \n",
    "\n",
    "    def printinfo(self): #メゾットの定義\n",
    "        print(self.name)\n",
    "        print(self.age)\n",
    "\n",
    "human = Human(\"太郎\", 20) #クラスのインスタンスを作成\n",
    "\n",
    "human.printinfo() #メソッドを呼び出し"
   ]
  }
